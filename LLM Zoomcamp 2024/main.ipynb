    "from tqdm import tqdm\n",
    "point = 0\n",
    "\n",
    "df = pd.DataFrame(columns=['problem_id', 'answer'])\n",
    "for i in tqdm(range(test.shape[0])):\n",
    "    ans = generate(prompt_answer.format(test.iloc[i][\"problem_text\"]))\n",
    "    #print(ans)\n",
    "    ans_2 = generate(parse_answer(test.iloc[i][\"problem_text\"], ans))\n",
    "    ans_3 = generate(parse_answer(test.iloc[i][\"problem_text\"], ans_2))\n",
    "    res = extract_numerical_answer(ans_3)\n",
    "    result = {\"problem_id\": test.iloc[i][\"problem_id\"], \"answer\": res}\n",
    "    df = pd.concat([df,pd.DataFrame([result])],ignore_index=True)\n",
    "    if(i == 0):\n",
    "        df.head(1)\n",
    "df.to_csv(\"ans.csv\", index=False)"
   ]
